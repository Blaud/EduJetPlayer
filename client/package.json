{
  "name": "edujetplayer",
  "version": "1.2.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --build-optimizer=false --aot=false",
    "buildAsItShouldBe": "ng build --prod --build-optimizer=false --aot=false",
    "test": "ng test",
    "test:coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint client --fix && ng lint client-e2e --fix",
    "e2e": "ng e2e",
    "i18n:extract": "ng xi18n --output-path locale",
    "electron": "ng build && electron ."
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/animations": "^6.1.0",
    "@angular/cli": "~6.1.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "angular-particle": "^1.0.4",
    "angular-particle-effect-button": "0.0.2",
    "animejs": "^2.2.0",
    "core-js": "^2.5.4",
    "dashjs": "^2.9.3",
    "hls.js": "^0.12.4",
    "materialize-css": "^1.0.0-rc.2",
    "rxjs": "^6.0.0",
    "srt-webvtt": "^1.0.1",
    "subtitle": "^2.0.3",
    "typescript": "~2.7.2",
    "videogular2": "^6.4.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular/language-service": "^6.1.0",
    "@types/core-js": "^2.5.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "electron": "^5.0.7",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}
